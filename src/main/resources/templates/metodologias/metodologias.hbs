{{#partial "contenido"}}
<h1>Metodologias</h1>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Link para evaluar</th>
		<th></th>
	</tr>
	
	{{#each metodologias}}
		<tr>
			<td>{{nombre}}</td>
			<td><a href="/metodologias/{{nombre}}/evaluar"><button>Evaluar</button></a></td>
		</tr>
	{{/each}}
</table>
{{/partial}}

{{>layout.hbs}}
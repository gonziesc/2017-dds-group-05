{{#partial "contenido"}}
<h1>Empresas</h1>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Link a cuentas</th>
		<th></th>
	</tr>
	
	{{#each empresas}}
		<tr>
			<td>{{nombreEmpresa}}</td>
			<td><a href="/empresas/{{nombreEmpresa}}/cuentas"><button>Cuentas</button></a></td>
		</tr>
	{{/each}}
</table>
{{/partial}}

{{>layout.hbs}}